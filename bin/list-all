#!/usr/bin/env bash

set -eu -o pipefail

# shellcheck source=lib/utils.sh
source "$(dirname "$0")/../lib/utils.sh"

sort_versions() {
  sed 'h; s/[+-]/./g; s/.p\([[:digit:]]\)/.z\1/; s/$/.z/; G; s/\n/ /' |
    LC_ALL=C sort -t. -k 1,1 -k 2,2n -k 3,3n -k 4,4n -k 5,5n | awk '{print $2}'
}

versions=$(
  phpbuild_wrapped --definitions |
    sort_versions |
    xargs
)

echo "$versions"
